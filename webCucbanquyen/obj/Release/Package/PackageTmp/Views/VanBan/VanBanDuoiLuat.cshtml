@using CucbanquyenModel.Models
@{
    var eType = (IEnumerable<DocumentType>)ViewBag.eType;
}
@model IEnumerable<Document>
@if (Model != null && Model.Count() > 0)
{
    foreach (var item in Model)
    {
        <tr>
            <td class="text-danger">@item.documentName</td>
            <td class="text-danger">@item.issuedTime.ToString("dd-MM-yyyy")</td>
            <td>
                <a href="@item.documentUrl">@item.documentNote</a>
            </td>
        </tr>
    }
}

